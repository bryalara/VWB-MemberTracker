<%= render "layouts/navbar2", user: @user %>
<div class="header">
<h2 class="active h2-title">Delete <%= @point_event.name %>?</h2>
</div>
<button class="btn align-center" style="float:right; margin-right: 20px;"> <%= link_to "Back to Events / Engagements", event_index_path, class:"back-button" %> </button>

<%= render "form", point_event: @point_event %>

<button class="btn btn-decor" style="float:right; margin-right: 70px;"><td><%= link_to "Delete", point_event_path(@point_event), method: :delete, data: {confirm: "Are you sure you want to delete \"#{@point_event.name}\"?\nOnce deleted, the data will be lost forever. Points that students received from this engagement will also dissappear!"}, class:"back-button", id: "delete-btn" %></td></button>
