<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>Veterinarians Without Borders</title>

    <!-- Font Awesome icons (free version)-->
    <script src="https://use.fontawesome.com/releases/v5.15.1/js/all.js" crossorigin="anonymous"></script>
    <!-- Google fonts-->
    <link href="https://fonts.googleapis.com/css?family=Merriweather+Sans:400,700" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic" rel="stylesheet" type="text/css" />
    <!-- Third party plugin CSS-->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/magnific-popup.min.css" rel="stylesheet" />
    <link rel="preconnect" href="https://fonts.gstatic.com">
  <link href="https://fonts.googleapis.com/css2?family=Homenaje&display=swap" rel="stylesheet">

</head>

<body id="page-top">

  <!-- Navigation -->
  <nav class="navbar navbar-expand-lg navbar-dark bg dark fixed-top" id="mainNav" style="background-color: #222222;">
    <div class="container">
      <a class="navbar-brand js-scroll-trigger" href="#page-top">VWB</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarResponsive">
        <ul class="navbar-nav ml-auto">
          <li class="nav-item">
            <a class="nav-link js-scroll-trigger" href="#about">About</a>
          </li>
          <li class="nav-item">
            <a class="nav-link js-scroll-trigger" href="#services">Service</a>
          </li>
          <li class="nav-item">
            <a class="nav-link js-scroll-trigger" href="#contact">Contact Us</a>
          </li>
              <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="<%= memberDashboard_path%>">Member</a>
              </li> 
          <%# <% if userlogin_signed_in? %>
            <%# <% if user.role == 0 %>
              <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="<%= users_path %>">Admin</a>
              </li> 
            <%# <%end%>
          <%# <%end%>
          <% if !userlogin_signed_in? %>
              <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="<%= userlogin_google_oauth2_omniauth_authorize_path %>"><i class="fas fa-sign-in-alt"></i> Sign In</a>
              </li>
          <%end%>
          <% if userlogin_signed_in? %>
              <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="/userlogins/sign_out"><i class="fas fa-sign-out-alt"></i> Log Out</a>
              </li>
          <%end%>
        </ul>
      </div>
    </div>
  </nav>
<div class="title-bar" style="background-color: white;">
  <header class="bg-primary text-white" style="padding-left:0px; padding-right: 0px;" style="background-color: white;">
    <div class=" text-center title-bar">
      <h1 class="title">Veterinarians Without Borders</h1>
      <p class="lead title">Texas A&M University</p>
    </div>
  </header>
  </div>

  <section id="about">
    <div class="container">
      <div class="row">
        <div class="col-lg-8 mx-auto">
          <h2 id="aboutH">About Us</h2>

          <% Edithomepage.all.each_with_index  do |edithomepage, i| %>
        
            <% if edithomepage.name == "about_us" %>
              <td><%= (edithomepage.description) %></td>
            <% else %>
              <%  "Wrong Spelling of website attribute" %>
            <% end %>
    
          <% end %>

          <p class="lead"></p>
          <ul>
          </ul>
        </div>
      </div>
    </div>
  </section>

  <section id="services" class="bg-light">
    <div class="container">
      <div class="row">
        <div class="col-lg-8 mx-auto">
          <h2 id="serviceH">Service</h2>
        </div>
      </div>
    </div>
  </section>

  <section id="contact">
    <div class="container">
      <div class="row justify-content-center">
            <div class="col-lg-8 text-center">
                <h2 id="contactH" class="mt-0">Contact Us!</h2>
                <hr class="divider my-4" />
                <p class="text-muted mb-5">Stay up to date with the happenings at Veterinarians Without Borders by following our social medias and contacting us through our email!</p>
            </div>
        </div>
            <div class="row">
            <div class="col-lg-4 ml-auto text-center mb-5 mb-lg-0">
                <a class="d-block" href="https://www.facebook.com/vwbtamu/"><i class="fab fa-facebook-f fa-3x mb-3 text-muted"></i></a>
                <div>Veterinarians Without Borders- Texas A&M</div>
            </div>
            <div class="col-lg-4 mr-auto text-center">
                <a class="d-block" href="mailto:vetswithoutborders@tamu.edu"><i class="fas fa-envelope fa-3x mb-3 text-muted"></i></a>
                <div>vetswithoutborders@tamu.edu</div>
            </div>
            <div class="col-lg-4 ml-auto text-center mb-5 mb-lg-0">
                <a class="d-block" href="https://www.instagram.com/vwbtamu/?hl=en"><i class="fab fa-instagram fa-3x mb-3 text-muted"></i></a>
                <div>@vwbtamu</div>
            </div>
        </div>
    </div>
  </section>

<%= render "layouts/footer"%>
          <!-- Bootstrap core JS-->
        <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js"></script>
</body>

</html>
