<h1>Users</h1>

<ul>
    <% @users.each do |user| %>
        <li> 
            <%= link_to user.firstName+" "+user.lastName, user %> 
        </li>
        
        <li> approved: <%= user.approved %> </li>
    <% end %>
</ul>

<%= link_to "Create New User", new_user_path %>

<%= link_to "View All Users", users_path %>
