<%= flash[:notice]%>

<h1>Users</h1>

<ul>
    <% @users.each do |user| %>
        <li> 
            <%= link_to user.firstName+" "+user.lastName, user %> 
        </li>
    <% end %>
</ul>

<div>
    <h4> Import data from csv file </h4>
        <%= form_tag import_users_path, multipart: true do %>
            <%= file_field_tag :file %>
            <%= submit_tag "Import CSV" %>
        <% end %>
</div>

<%= link_to "Create New User", new_user_path %>

<%= link_to "View Pending Users", pendingApproval_path %>