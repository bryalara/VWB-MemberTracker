<h1>Points Events</h1>
<%= link_to "Add a Points Event", new_point_event_path(@pointEvents) %>
<table>
	<tr>
		<th>Event</th>
		<th>Description</th>
		<th>Points</th>
		<th>Actions</th>
	</tr>

	<% @pointEvents.each do |pointEvent| %>
	<tr>
		<td><%= pointEvent.name %></td>
		<td><%= pointEvent.description %></td>
		<td><%= pointEvent.points %></td>

		<td><%= link_to "Show", point_event_path(pointEvent) %></td>
		<td><%= link_to "Edit", edit_point_event_path(pointEvent) %></td>
		<td><%= link_to "Delete", delete_point_event_path(pointEvent) %></td>
		<td><%= link_to "QR", qr_point_event_path(pointEvent) %></td>
	</tr>
	<% end %>
</table>
